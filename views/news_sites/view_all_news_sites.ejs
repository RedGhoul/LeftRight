<%- include ../partials/nav.ejs %>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Url</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            <% for(var i=0; i < data.length; i++) { %>
                <tr>
                    <td>
                        <%= data[i].name %>
                    </td>
                    <td>
                        <%= data[i].url %>
                    </td>
                    <td>
                        <a href="/NewsSites/Delete/<%= data[i].id %>" class="btn btn-primary">Delete</a>
                        <a href="/NewsSites/Update/<%= data[i].id %>" class="btn btn-primary">Update</a>
                    </td>
                </tr>
                <% } %>

        </tbody>
    </table>
